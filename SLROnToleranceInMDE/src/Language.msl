metamodel Bibliography {
	Author {
		.firstName : EString
		.lastName  : EString
	}	
	
	Paper {
		-authors(0..*)->Author
		-cites(0..*)->Paper
		.title : EString
		.core : EBoolean
		.relevant : EBoolean		
		.year  : EInt
		-venue(1..*) ->Venue
	}
	
	Venue {
		.Name : EString
		.SE   : EBoolean
		
	}	
	
}